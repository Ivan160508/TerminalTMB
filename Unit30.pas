procedure TFormStat.tmrReadTimer(Sender: TObject);
  var
    i: Integer;
    sum : Cardinal;
    pBufCntRead, pBufCntSend : PCardinal;
begin
    sum := 0;

    if isReadStat then
      begin
        pBufCntRead := BufRead;
        for i:= 0 to 255 do
          begin
            if CNameByteSt[i].Caption <> IntToStr(pBufCntRead^) then
              begin
                CNameByteSt[i].Caption := IntToStr(pBufCntRead^);
                if isSelectColor[i] then
                  CNameByteSt[i].Color := clLime
                else
                  CNameByteSt[i].Color := clYellow;
              end
            else
              if isSelectColor[i] then
                CNameByteSt[i].Color := clAqua
              else
                CNameByteSt[i].Color := clWindow;

            sum := sum + pBufCntRead^;
            inc(pBufCntRead, 1);
          end;
      end
    else
      begin
        pBufCntSend := BufSend;
        for i:= 0 to 255 do
          begin
            if CNameByteSt[i].Caption <> IntToStr(pBufCntSend^) then
              begin
                CNameByteSt[i].Caption := IntToStr(pBufCntSend^);
                if isSelectColor[i] then
                  CNameByteSt[i].Color := clAqua
                else
                  CNameByteSt[i].Color := clYellow;
              end
            else
              if isSelectColor[i] then
                CNameByteSt[i].Color := clLime
              else
                CNameByteSt[i].Color := clWindow;

            sum := sum + pBufCntSend^;
            inc(pBufCntSend, 1);
          end;
      end;
    ctxt257.Caption := IntToStr(sum);
end;

procedure TFormStat.btnResetClick(Sender: TObject);
  var
    i: Integer;
    pBufCntRead, pBufCntSend : PCardinal;

  begin
    pBufCntRead := BufRead;
    pBufCntSend := BufSend;
    for i:= 0 to 255 do
      begin
        if isReadStat then
          pBufCntRead^ := 0
        else
          pBufCntSend^ := 0;

        inc(pBufCntRead, 1);
        inc(pBufCntSend, 1);
      end;
  end;

