function dec_to_bin(dec: Integer): Integer;
var
  bin, rank, modulo: Integer;
begin
  bin := 0;
  rank := 1;
  while dec > 0 do
  begin
    modulo := dec mod 2;
    dec := dec div 2;
    bin := bin + modulo * rank;
    rank := rank * 10;
  end;
  result := bin;
end;


