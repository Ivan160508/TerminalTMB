procedure TFormAsciiTable.FormClose(Sender: TObject;
  var Action: TCloseAction);
begin
  TerminalST.Form1.BtBTASCIITABLE.Font.Style := [];
end;

procedure TFormAsciiTable.AsciiClick(Sender: TObject);
begin
  if isSelect then
    begin
      ByteClick := (Sender as TStaticText).Tag;
      isSelect := False;
      Help := (Sender as TStaticText).Caption;
      FormAsciiTable.Close;
    end;
end;

procedure TFormAsciiTable.FormActivate(Sender: TObject);
var
  i : Integer;
begin
  if isSelect then
    begin
      For i := 0 to 255 do
        SymbolASCII[i].Color := clWindow;
      if ByteClick < 256 then
        SymbolASCII[ByteClick].Color := clYellow;
      Help := '';
    end;
end;

